{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.job.com/job.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "id": {
      "type": "integer",
      "minLength": 1,
      "maxLength": 10
    },
    "salary": {
      "type": "integer",
      "minimum": 0
    },
    "equity": {
      "type": "integer",
      "minimum": 0
    },
    "companyHandle": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": ["id", "title", "companyHandle"]
}
